[org.osg.general.osg-directories-CE-permissions]
default-cron-interval = 37 * * * *

execute = local
service-type = OSG-CE
output-format = wlcg
probe-spec = v3


[org.osg.general.osg-directories-CE-permissions env]
PERL5LIB = PREPEND | !!VDT_LOCATION!!/osg-rsv/bin/probes:!!VDT_PERL5LIB!!
LD_LIBRARY_PATH = APPEND | !!VDT_LOCATION!!/globus/lib
GLOBUS_LOCATION = SET | !!VDT_LOCATION!!/globus
